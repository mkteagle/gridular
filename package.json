{
  "name": "gridular",
  "version": "3.3.2",
  "description": "A high-performance virtualized React data grid with grouping, sorting, filtering, and comprehensive customization",
  "type": "module",
  "main": "./dist/virtualized-grid.js",
  "module": "./dist/virtualized-grid.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/virtualized-grid.js"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "pnpm run build",
    "publish:login": "npm login",
    "publish:npm": "npm publish",
    "publish:beta": "npm publish --tag beta",
    "publish:next": "npm publish --tag next"
  },
  "keywords": [
    "react",
    "virtualized",
    "grid",
    "datagrid",
    "table",
    "tanstack",
    "virtual",
    "component",
    "grouping",
    "sorting",
    "filtering"
  ],
  "author": "Michael Teagle",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mkteagle/gridular.git"
  },
  "homepage": "https://gridular.vercel.app",
  "packageManager": "pnpm@9.0.0",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@tanstack/react-virtual": "^3.13.13",
    "clsx": "^2.1.1",
    "lucide-react": "^0.559.0",
    "tailwind-merge": "^3.4.0",
    "tss-react": "^4.9.20"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@storybook/addon-a11y": "^10.1.6",
    "@storybook/addon-docs": "^10.1.6",
    "@storybook/addon-onboarding": "^10.1.6",
    "@storybook/addon-vitest": "^10.1.6",
    "@storybook/react": "^10.1.6",
    "@storybook/react-vite": "^10.1.6",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "4.0.15",
    "autoprefixer": "^10.4.22",
    "jsdom": "^27.3.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "storybook": "^10.1.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.15"
  }
}
