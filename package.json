{
  "name": "gridular",
  "version": "1.0.1",
  "description": "A customizable data grid component for React.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkteagle/gridular"
  },
  "homepage": "https://gridular.vercel.app",
  "bugs": {
    "url": "https://github.com/mkteagle/gridular/issues"
  },
  "author": "Michael Teagle",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "tsup components/index.ts --format esm,cjs --dts",
    "dev-tsup": "tsup components/index.ts --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "publish": "npm publish --access public"
  },
  "keywords": [
    "react",
    "data-grid",
    "table",
    "pagination",
    "sorting",
    "filtering"
  ],
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.4.7",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.482.0",
    "next": "^15.2.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tss-react": "^4.9.15"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.7",
    "@storybook/addon-links": "^8.6.7",
    "@storybook/nextjs": "^8.6.6",
    "@storybook/react": "^8.6.6",
    "@storybook/test": "^8.6.6",
    "@storybook/theming": "^8.6.7",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "3.0.9",
    "jsdom": "^26.0.0",
    "storybook": "^8.6.6",
    "storybook-dark-mode": "^4.0.2",
    "tailwindcss": "^4",
    "tsup": "^8.4.0",
    "typescript": "^5",
    "vitest": "^3.0.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "string-width": "4.2.3",
    "strip-ansi": "6.0.1",
    "wrap-ansi": "7.0.0",
    "ansi-regex": "5.0.1",
    "jackspeak": "2.1.1"
  },
  "overrides": {
    "string-width": "4.2.3",
    "strip-ansi": "6.0.1",
    "wrap-ansi": "7.0.0",
    "ansi-regex": "5.0.1",
    "jackspeak": "2.1.1"
  },
  "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72"
}
